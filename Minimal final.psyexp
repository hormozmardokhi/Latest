      <CodeComponent name="code_stimuli" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="# Define the possible options&amp;#10;shapes = [&quot;diamond&quot;, &quot;circle&quot;, &quot;wavey&quot;]&amp;#10;colors = [&quot;green&quot;, &quot;red&quot;, &quot;blue&quot;]&amp;#10;quantities = [1, 2, 3]&amp;#10;textures = [&quot;filled&quot;, &quot;blank&quot;, ]&amp;#10;&amp;#10;# Declare global variables&amp;#10;chosen_shape = None&amp;#10;chosen_color = None&amp;#10;chosen_quantity = None&amp;#10;chosen_texture = None&amp;#10;&amp;#10;# List to collect data&amp;#10;data_collection = []&amp;#10;&amp;#10;&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="shapes = [&quot;diamond&quot;, &quot;circle&quot;, &quot;wavey&quot;];&amp;#10;colors = [&quot;green&quot;, &quot;red&quot;, &quot;blue&quot;];&amp;#10;quantities = [1, 2, 3];&amp;#10;textures = [&quot;filled&quot;, &quot;blank&quot;];&amp;#10;chosen_shape = null;&amp;#10;chosen_color = null;&amp;#10;chosen_quantity = null;&amp;#10;chosen_texture = null;&amp;#10;data_collection = [];&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="import * as random from 'random';&amp;#10;chosen_shape = Math.random.choice(shapes);&amp;#10;chosen_color = Math.random.choice(colors);&amp;#10;chosen_quantity = Math.random.choice(quantities);&amp;#10;chosen_texture = Math.random.choice(textures);&amp;#10;fixed_y = 0;&amp;#10;shape_size = Math.max(100, 150);&amp;#10;base_spacing = (shape_size * 1.5);&amp;#10;positions = [];&amp;#10;if ((chosen_quantity === 1)) {&amp;#10;    positions = [[0, fixed_y]];&amp;#10;} else {&amp;#10;    if ((chosen_quantity === 2)) {&amp;#10;        spacing = (base_spacing * 1.2);&amp;#10;        positions = [[(- spacing), fixed_y], [spacing, fixed_y]];&amp;#10;    } else {&amp;#10;        if ((chosen_quantity === 3)) {&amp;#10;            spacing = (base_spacing * 1.2);&amp;#10;            positions = [[(- spacing), fixed_y], [0, fixed_y], [spacing, fixed_y]];&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;if ((chosen_shape === &quot;diamond&quot;)) {&amp;#10;    circle_shape.opacity = 0;&amp;#10;    wavey_shape.opacity = 0;&amp;#10;    diamond_shape.opacity = 1;&amp;#10;    diamond_shape.fillColor = ((chosen_texture === &quot;filled&quot;) ? chosen_color : null);&amp;#10;    diamond_shape.lineColor = ((chosen_texture === &quot;blank&quot;) ? chosen_color : null);&amp;#10;    diamond_shape.lineWidth = ((chosen_texture === &quot;filled&quot;) ? 0 : 2);&amp;#10;    for (var i, _pj_c = 0, _pj_a = util.range(chosen_quantity), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;        i = _pj_a[_pj_c];&amp;#10;        diamond_shape.pos = positions[i];&amp;#10;        diamond_shape.draw();&amp;#10;    }&amp;#10;} else {&amp;#10;    if ((chosen_shape === &quot;circle&quot;)) {&amp;#10;        diamond_shape.opacity = 0;&amp;#10;        wavey_shape.opacity = 0;&amp;#10;        circle_shape.opacity = 1;&amp;#10;        circle_shape.fillColor = ((chosen_texture === &quot;filled&quot;) ? chosen_color : null);&amp;#10;        circle_shape.lineColor = ((chosen_texture === &quot;blank&quot;) ? chosen_color : null);&amp;#10;        circle_shape.lineWidth = ((chosen_texture === &quot;filled&quot;) ? 0 : 2);&amp;#10;        for (var i, _pj_c = 0, _pj_a = util.range(chosen_quantity), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;            i = _pj_a[_pj_c];&amp;#10;            circle_shape.pos = positions[i];&amp;#10;            circle_shape.draw();&amp;#10;        }&amp;#10;    } else {&amp;#10;        if ((chosen_shape === &quot;wavey&quot;)) {&amp;#10;            diamond_shape.opacity = 0;&amp;#10;            circle_shape.opacity = 0;&amp;#10;            wavey_shape.opacity = 1;&amp;#10;            wavey_shape.fillColor = ((chosen_texture === &quot;filled&quot;) ? chosen_color : null);&amp;#10;            wavey_shape.lineColor = ((chosen_texture === &quot;blank&quot;) ? chosen_color : null);&amp;#10;            wavey_shape.lineWidth = ((chosen_texture === &quot;filled&quot;) ? 0 : 2);&amp;#10;            for (var i, _pj_c = 0, _pj_a = util.range(chosen_quantity), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;                i = _pj_a[_pj_c];&amp;#10;                wavey_shape.pos = positions[i];&amp;#10;                wavey_shape.draw();&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;psychoJS.window.flip();&amp;#10;core.wait(0.5);&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="import random&amp;#10;&amp;#10;# Randomly select shape, color, and quantity for each trial&amp;#10;chosen_shape = random.choice(shapes)&amp;#10;chosen_color = random.choice(colors)&amp;#10;chosen_quantity = random.choice(quantities)&amp;#10;chosen_texture = random.choice(textures)&amp;#10;&amp;#10;# Define fixed y-coordinate for all shapes&amp;#10;fixed_y = 0&amp;#10;&amp;#10;# Define base spacing based on the largest shape size&amp;#10;shape_size = max(100, 150)  # Max size of the circle or diamond&amp;#10;base_spacing = shape_size * 1.5  # Start with 1.5 times the size of the largest shape&amp;#10;&amp;#10;# Calculate positions based on the quantity, increasing spacing as needed&amp;#10;positions = []&amp;#10;if chosen_quantity == 1:&amp;#10;    positions = [(0, fixed_y)]  # Center position for one shape&amp;#10;elif chosen_quantity == 2:&amp;#10;    # Use larger spacing for two shapes&amp;#10;    spacing = base_spacing * 1.2  # Adjust to further increase space&amp;#10;    positions = [(-spacing, fixed_y), (spacing, fixed_y)]&amp;#10;elif chosen_quantity == 3:&amp;#10;    # Decrease spacing for three shapes to ensure they fit on screen&amp;#10;    spacing = base_spacing * 1.2  # Adjusted to fit within the screen&amp;#10;    positions = [(-spacing, fixed_y), (0, fixed_y), (spacing, fixed_y)]&amp;#10;&amp;#10;&amp;#10;# In each shape condition&amp;#10;if chosen_shape == &quot;diamond&quot;:&amp;#10;    circle_shape.opacity = 0&amp;#10;    wavey_shape.opacity = 0&amp;#10;    diamond_shape.opacity = 1&amp;#10;    diamond_shape.fillColor = chosen_color if chosen_texture == &quot;filled&quot; else None&amp;#10;    diamond_shape.lineColor = chosen_color if chosen_texture == &quot;blank&quot; else None&amp;#10;    diamond_shape.lineWidth = 0 if chosen_texture == &quot;filled&quot; else 2  # No border for filled shapes&amp;#10;&amp;#10;    for i in range(chosen_quantity):&amp;#10;        diamond_shape.pos = positions[i]&amp;#10;        diamond_shape.draw()&amp;#10;&amp;#10;elif chosen_shape == &quot;circle&quot;:&amp;#10;    diamond_shape.opacity = 0&amp;#10;    wavey_shape.opacity = 0&amp;#10;    circle_shape.opacity = 1&amp;#10;    circle_shape.fillColor = chosen_color if chosen_texture == &quot;filled&quot; else None&amp;#10;    circle_shape.lineColor = chosen_color if chosen_texture == &quot;blank&quot; else None&amp;#10;    circle_shape.lineWidth = 0 if chosen_texture == &quot;filled&quot; else 2&amp;#10;&amp;#10;    for i in range(chosen_quantity):&amp;#10;        circle_shape.pos = positions[i]&amp;#10;        circle_shape.draw()&amp;#10;&amp;#10;elif chosen_shape == &quot;wavey&quot;:&amp;#10;    diamond_shape.opacity = 0&amp;#10;    circle_shape.opacity = 0&amp;#10;    wavey_shape.opacity = 1&amp;#10;    wavey_shape.fillColor = chosen_color if chosen_texture == &quot;filled&quot; else None&amp;#10;    wavey_shape.lineColor = chosen_color if chosen_texture == &quot;blank&quot; else None&amp;#10;    wavey_shape.lineWidth = 0 if chosen_texture == &quot;filled&quot; else 2&amp;#10;&amp;#10;    for i in range(chosen_quantity):&amp;#10;        wavey_shape.pos = positions[i]&amp;#10;        wavey_shape.draw()&amp;#10;&amp;#10;&amp;#10;&amp;#10;win.flip()  # Update the display&amp;#10;core.wait(0.5)  # Wait for 500ms&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
