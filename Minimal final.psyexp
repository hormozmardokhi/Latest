        <Param val="continueRoutine = true;&amp;#10;response_counter = 0;&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# Begin Routine tab&amp;#10;continueRoutine = True&amp;#10;response_counter = 0  # Ensure response counter resets each time&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="&amp;#10;import math&amp;#10;&amp;#10;# Each frame&amp;#10;mouse_buttons = mouse.getPressed()&amp;#10;&amp;#10;response_counter = 0  # Reset the response counter for each routine&amp;#10;max_responses = 1     # Define max responses to control when the routine ends&amp;#10;&amp;#10;# Initialize variables at the start of each frame to ensure they have default values&amp;#10;region = &quot;None&quot;&amp;#10;color = &quot;None&quot;&amp;#10;quadrant = &quot;Unknown Quadrant&quot;  # Initialize quadrant with a default value&amp;#10;&amp;#10;# Only proceed if the left mouse button is pressed and we haven't reached max responses&amp;#10;if mouse_buttons[0] and response_counter &lt; max_responses:&amp;#10;    # Check if the mouse clicked on any of the circles&amp;#10;    valid_click = False  # Assume the click is invalid initially&amp;#10;&amp;#10;    if mouse.isPressedIn(inner_circle):&amp;#10;        region = &quot;inner region&quot;&amp;#10;        color = &quot;Red&quot;&amp;#10;        valid_click = True&amp;#10;    elif mouse.isPressedIn(middle_circle):&amp;#10;        region = &quot;middle region&quot;&amp;#10;        color = &quot;Blue&quot;&amp;#10;        valid_click = True&amp;#10;    elif mouse.isPressedIn(outer_circle):&amp;#10;        region = &quot;outer region&quot;&amp;#10;        color = &quot;Green&quot;&amp;#10;        valid_click = True&amp;#10;    &amp;#10;    # Only log data and increment counter if the click was valid (inside a circle)&amp;#10;    if valid_click:&amp;#10;        # Increment the response counter for valid clicks&amp;#10;        response_counter += 1&amp;#10;&amp;#10;        # End routine if max responses reached&amp;#10;        if response_counter &gt;= max_responses:&amp;#10;            continueRoutine = False&amp;#10;            print(&quot;Routine ending: Valid click within a circle and max responses reached.&quot;)&amp;#10;    else:&amp;#10;        # Click was outside any circle; no action taken, routine continues&amp;#10;        print(&quot;Click outside the circles; routine continues.&quot;)&amp;#10; &amp;#10;    # Get the mouse position&amp;#10;    mouse_pos = mouse.getPos()&amp;#10;    radius = (mouse_pos[0]**2 + mouse_pos[1]**2)**0.5&amp;#10;&amp;#10;    # Calculate the angle of the click&amp;#10;    angle = math.degrees(math.atan2(mouse_pos[1], mouse_pos[0]))&amp;#10;    if angle &lt; 0:&amp;#10;        angle += 360  # Normalize angle to be between 0 and 360 degrees&amp;#10;&amp;#10;    # Assign quadrant numbers based on 90-degree angle ranges&amp;#10;    if 0 &lt;= angle &lt; 90:&amp;#10;        quadrant = 1&amp;#10;    elif 90 &lt;= angle &lt; 180:&amp;#10;        quadrant = 2&amp;#10;    elif 180 &lt;= angle &lt; 270:&amp;#10;        quadrant = 3&amp;#10;    elif 270 &lt;= angle &lt; 360:&amp;#10;        quadrant = 4&amp;#10;&amp;#10;    # Log all relevant data for each frame, regardless of whether a valid click occurred&amp;#10;    thisExp.addData(&quot;radius&quot;, radius)&amp;#10;    thisExp.addData(&quot;angle&quot;, angle)&amp;#10;    thisExp.addData(&quot;region&quot;, region)&amp;#10;    thisExp.addData(&quot;color&quot;, color)&amp;#10;    thisExp.addData(&quot;quadrant&quot;, quadrant)&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="import * as math from 'math';&amp;#10;if (((trial_counter % 2) === 1)) {&amp;#10;    continueRoutine = false;&amp;#10;}&amp;#10;mouse_buttons = mouse.getPressed();&amp;#10;response_counter = 0;&amp;#10;max_responses = 1;&amp;#10;region = &quot;None&quot;;&amp;#10;color = &quot;None&quot;;&amp;#10;quadrant = &quot;Unknown Quadrant&quot;;&amp;#10;if ((mouse_buttons[0] &amp;&amp; (response_counter &lt; max_responses))) {&amp;#10;    valid_click = false;&amp;#10;    if (mouse.isPressedIn(inner_circle)) {&amp;#10;        region = &quot;inner region&quot;;&amp;#10;        color = &quot;Red&quot;;&amp;#10;        valid_click = true;&amp;#10;    } else {&amp;#10;        if (mouse.isPressedIn(middle_circle)) {&amp;#10;            region = &quot;middle region&quot;;&amp;#10;            color = &quot;Blue&quot;;&amp;#10;            valid_click = true;&amp;#10;        } else {&amp;#10;            if (mouse.isPressedIn(outer_circle)) {&amp;#10;                region = &quot;outer region&quot;;&amp;#10;                color = &quot;Green&quot;;&amp;#10;                valid_click = true;&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;    if (valid_click) {&amp;#10;        response_counter += 1;&amp;#10;        if ((response_counter &gt;= max_responses)) {&amp;#10;            continueRoutine = false;&amp;#10;            console.log(&quot;Routine ending: Valid click within a circle and max responses reached.&quot;);&amp;#10;        }&amp;#10;    } else {&amp;#10;        console.log(&quot;Click outside the circles; routine continues.&quot;);&amp;#10;    }&amp;#10;    mouse_pos = mouse.getPos();&amp;#10;    radius = Math.pow((Math.pow(mouse_pos[0], 2) + Math.pow(mouse_pos[1], 2)), 0.5);&amp;#10;    angle = math.degrees(Math.atan2(mouse_pos[1], mouse_pos[0]));&amp;#10;    if ((angle &lt; 0)) {&amp;#10;        angle += 360;&amp;#10;    }&amp;#10;    if (((0 &lt;= angle) &amp;&amp; (angle &lt; 90))) {&amp;#10;        quadrant = 1;&amp;#10;    } else {&amp;#10;        if (((90 &lt;= angle) &amp;&amp; (angle &lt; 180))) {&amp;#10;            quadrant = 2;&amp;#10;        } else {&amp;#10;            if (((180 &lt;= angle) &amp;&amp; (angle &lt; 270))) {&amp;#10;                quadrant = 3;&amp;#10;            } else {&amp;#10;                if (((270 &lt;= angle) &amp;&amp; (angle &lt; 360))) {&amp;#10;                    quadrant = 4;&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;    psychoJS.experiment.addData(&quot;radius&quot;, radius);&amp;#10;    psychoJS.experiment.addData(&quot;angle&quot;, angle);&amp;#10;    psychoJS.experiment.addData(&quot;region&quot;, region);&amp;#10;    psychoJS.experiment.addData(&quot;color&quot;, color);&amp;#10;    psychoJS.experiment.addData(&quot;quadrant&quot;, quadrant);&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
